@model TreasureCache.Presentation.ViewModels.Administration.AdminPanelViewModel

<div class="grid gap-8 grid-cols-3 w-full">
    <div class="flex flex-col w-[250px] rounded-lg bg-gray-800 dark:hover:bg-gray-700 shadow-md shadow-black dark:border-gray-800">
        <a href="#">
            <img class="rounded-t-lg w-[100%] mx-auto" src="~/assets/icons/user-icon.png" alt="users icon" />
            <div class="mt-4 px-5 pb-5 grow flex flex-col justify-between">
                <h5 class="text-xl font-semibold tracking-tight text-gray-900 dark:text-white" align="center">Users</h5>
            </div>
        </a>
    </div>
    <div class="flex flex-col w-[250px] rounded-lg bg-gray-800 dark:hover:bg-gray-700 shadow-md shadow-black dark:border-gray-800">
        <a href="#">
            <img class="rounded-t-lg w-[100%] mx-auto" src="~/assets/icons/shopping-cart-icon-illustration-free-vector.jpg" alt="products icon" />
            <div class="mt-4 px-5 pb-5 grow flex flex-col justify-between">
                <h5 class="text-xl font-semibold tracking-tight text-gray-900 dark:text-white" align="center">Products</h5>
            </div>
        </a>
    </div>
    <div class="flex flex-col w-[250px] rounded-lg bg-gray-800 dark:hover:bg-gray-700 shadow-md shadow-black dark:border-gray-800">
        <a href="#">
            <img class="rounded-t-lg w-[100%] mx-auto" src="~/assets/icons/writing-form-icon-outline-user-online-form-vector.jpg" alt="contact form icon" />
            <div class="mt-4 px-5 pb-5 grow flex flex-col justify-between">
                <h5 class="text-xl font-semibold tracking-tight text-gray-900 dark:text-white" align="center">Contact forms</h5>
            </div>
        </a>
    </div>
</div>
